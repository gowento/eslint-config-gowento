{
  "name": "eslint-config-gowento",
  "version": "1.2.0",
  "description": "Shared ESLint config used in Gowento projects.",
  "contributors": [
    "Loris Guignard <loris.guignard@gmail.com>"
  ],
  "main": "index.js",
  "scripts": {
    "find-unused-rules": "eslint-find-rules -u index.js",
    "find-unused-rules:react": "eslint-find-rules -u react.js",
    "lint": "eslint . --cache",
    "deploy": "git pull --rebase origin master && git push origin master",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "postpublish": "git push origin master --follow-tags",
    "toc": "doctoc --github --title \"# Changelog\" CHANGELOG.md"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "files": [
    "index.js",
    "react.js"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:gowento/eslint-config-gowento.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/gowento/eslint-config-gowento#readme",
  "peerDependencies": {
    "babel-eslint": "6.0.4",
    "eslint": "2.10.2",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-ava": "2.4.0",
    "eslint-plugin-import": "1.8.1",
    "eslint-plugin-jsx-a11y": "1.2.2",
    "eslint-plugin-lodash": "1.9.0",
    "eslint-plugin-react": "5.1.1"
  },
  "dependencies": {
    "babel-eslint": "6.0.4",
    "eslint-config-airbnb": "9.0.1",
    "eslint-plugin-ava": "2.4.0",
    "eslint-plugin-import": "1.8.1",
    "eslint-plugin-jsx-a11y": "1.4.1",
    "eslint-plugin-lodash": "1.9.0",
    "eslint-plugin-react": "5.1.1"
  },
  "devDependencies": {
    "eslint": "2.10.2",
    "eslint-find-rules": "1.3.0"
  }
}
