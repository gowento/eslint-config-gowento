{
  "name": "eslint-config-gowento",
  "version": "4.1.0",
  "description": "Shared ESLint config used in Gowento projects.",
  "author": "Gowento <hackers@gowento.com>",
  "contributors": [
    "Loris Guignard <loris.guignard@gmail.com>",
    "Eric Redon <eric.redon@coox.org>"
  ],
  "main": "index.js",
  "scripts": {
    "find-unused-rules": "eslint-find-rules -u index.js",
    "lint": "eslint . --cache",
    "deploy": "git pull --rebase origin master && git push origin master",
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "postpublish": "git push origin master --follow-tags"
  },
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "files": [
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:gowento/eslint-config-gowento.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/gowento/eslint-config-gowento#readme",
  "dependencies": {
    "babel-eslint": "^7.2.3",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-ava": "^4.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-lodash": "^2.4.4",
    "eslint-plugin-react": "^7.1.0"
  },
  "devDependencies": {
    "eslint": "^3.19.0 || ^4.3.0"
  },
  "peerDependencies": {
    "eslint": "^3.19.0 || ^4.3.0"
  }
}
